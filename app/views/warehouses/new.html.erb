<div class="row">
  <div class="col-md-8">
    <h1>Warehouses#new</h1>

    <%= form_with(model: @warehouse, local: true) do |form| %>
      <% if @warehouse.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(@warehouse.errors.count, "error") %> prohibited this Warehouse from being saved:</h2>

          <ul>
            <% @warehouse.errors.full_messages.each do |message| %>
              <li><%= message %></li>
            <% end %>
          </ul>
        </div>
      <% end %>
      <div class="form-group row">
        <%= form.label :wh_code, class: 'col-2 col-form-label' %>
        <div class="col-8">
          <%= form.text_field :wh_code, class: 'form-control' %>
        </div>
      </div>
      <div class="form-group row">
        <%= form.label :name, class: 'col-2 col-form-label' %>
        <div class="col-8">
          <%= form.text_field :name, class: 'form-control' %>
        </div>
      </div>
      <div class="form-group row">
        <%= form.label :pincode, class: 'col-2 col-form-label' %>
        <div class="col-8">
          <%= form.text_field :pincode, class: 'form-control' %>
        </div>
      </div>
      <div class="form-group row">
        <%= form.label :max_capacity, class: 'col-2 col-form-label' %>
        <div class="col-8">
          <%= form.text_field :max_capacity, class: 'form-control' %>
        </div>
      </div>
      <div class="form-actions">
        <div class="row">
          <div class="col-md-2"></div>
          <div class="col-md-8">
            <%= form.submit "Create", class: 'btn btn-primary'%>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>

